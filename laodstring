-- Toggle Coin Farm
local coinFarmActive = false
local bodyVelocity = nil

local function toggleCoinFarm()
    coinFarmActive = not coinFarmActive
    if coinFarmActive then
        G2L["a"].TextColor3 = Color3.fromRGB(0, 255, 0) -- Green when active
        local player = game:GetService("Players").LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local hrp = character:WaitForChild("HumanoidRootPart")

        -- Enable noclip
        game:GetService("RunService").Stepped:Connect(function()
            for _, part in ipairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end)

        -- Create BodyVelocity for movement
        bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bodyVelocity.Velocity = Vector3.zero
        bodyVelocity.Parent = hrp

        -- Coin collection logic
        local visitedCoins = {}
        local function getCoins()
            local coins = {}
            for _, coin in ipairs(workspace:GetDescendants()) do
                if coin:IsA("MeshPart") and coin.Name == "MainCoin" and not visitedCoins[coin] then
                    table.insert(coins, coin)
                end
            end
            return coins
        end

        local function getClosestCoin()
            local coins = getCoins()
            local closestCoin = nil
            local shortestDistance = math.huge

            for _, coin in ipairs(coins) do
                local distance = (hrp.Position - coin.Position).Magnitude
                if distance < shortestDistance then
                    shortestDistance = distance
                    closestCoin = coin
                end
            end

            return closestCoin
        end

        while coinFarmActive do
            local coin = getClosestCoin()
            if coin then
                local targetPos = coin.Position
                local direction = (targetPos - hrp.Position).Unit
                bodyVelocity.Velocity = direction * 26
                if (hrp.Position - targetPos).Magnitude <= 1 then
                    visitedCoins[coin] = true
                    coin:Destroy()
                end
            else
                bodyVelocity.Velocity = Vector3.zero
            end
            task.wait()
        end
    else
        G2L["a"].TextColor3 = Color3.fromRGB(0, 0, 0) -- Black when inactive
        if bodyVelocity then
            bodyVelocity.Velocity = Vector3.zero
            bodyVelocity:Destroy()
            bodyVelocity = nil
        end
    end
end

G2L["a"].MouseButton1Click:Connect(toggleCoinFarm)

-- Anti-AFK
local afkExecuted = false

G2L["e"].MouseButton1Click:Connect(function()
    if afkExecuted then return end
    afkExecuted = true
    G2L["e"].TextColor3 = Color3.fromRGB(128, 0, 128) -- Purple when executed

    local virtualUser = game:GetService('VirtualUser')
    game:GetService('Players').LocalPlayer.Idled:Connect(function()
        virtualUser:CaptureController()
        virtualUser:ClickButton2(Vector2.new())
    end)
end)

-- GUI Persistence
local function onCharacterAdded(character)
    -- Reinitialize GUI if needed
end

game:GetService("Players").LocalPlayer.CharacterAdded:Connect(onCharacterAdded)

return G2L["1"], require
